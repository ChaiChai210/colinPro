<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/ll_item"
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dp_30"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_date"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:textColor="#ff6c686e"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/tv_directly_new"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:textColor="#ff6c686e"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/tv_directly_performance"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:textColor="#ff6c686e"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/tv_team_new"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:textColor="#ff6c686e"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/tv_team_performance"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:textColor="#ff6c686e"
            android:textSize="@dimen/sp_10" />

        <TextView
            android:id="@+id/tv_personal_money"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:textColor="#ff6c686e"
            android:textSize="@dimen/sp_10" />

        <RelativeLayout
            android:id="@+id/rl_check"
            android:layout_width="0.0dip"
            android:layout_height="fill_parent"
            android:layout_weight="1.0">

            <TextView
                android:layout_width="51.0dip"
                android:layout_height="17.0dip"
                android:layout_centerInParent="true"
                android:background="@drawable/shape_gray_5"
                android:gravity="center"
                android:padding="@dimen/dp_1"
                android:text="业绩来源"
                android:textColor="#ffa9a9ae"
                android:textSize="@dimen/sp_10" />
        </RelativeLayout>
    </LinearLayout>
</LinearLayout>