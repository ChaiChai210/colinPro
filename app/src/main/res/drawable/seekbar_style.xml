<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp_10" />
            <gradient android:startColor="#ff808a87" android:endColor="#ff808a87" android:angle="270.0" android:centerY="0.5" android:centerColor="#ff1a1918" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_10" />
                <gradient android:startColor="#ff775227" android:endColor="#ffffcc8d" android:angle="270.0" android:centerY="0.5" android:centerColor="#fff8c688" />
            </shape>
        </clip>
    </item>
</layer-list>